<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="attendee_list" meta_type="CMF Action">
   <property name="title">Attendee List</property>
   <property name="description"></property>
   <property name="url_expr">string:${object_url}/attendee_list</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:here.portal_type == 'ILOEvent'</property>
   <property name="permissions">
    <element value="Change portal topics"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

</object>
